// Place your settings in this file to overwrite the default settings
{
  "cSpell.userWords": [
    "EMBEDLY",
    "arity",
    "cyclomatic",
    "django",
    "fullscreen",
    "minimap",
    "params",
    "permalink",
    "pycache",
    "reindex",
    "repo",
    "rtype",
    "slugify",
    "tzinfo"
  ],
  "diffEditor.ignoreTrimWhitespace": true,
  "editor.fontFamily": "'Source Code Pro'",
  "editor.fontSize": 13,
  "editor.formatOnSave": true,
  "editor.minimap.enabled": true,
  "editor.minimap.renderCharacters": false,
  "editor.renderIndentGuides": true,
  "editor.renderLineHighlight": "all",
  "editor.rulers": [70, 79],
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "files.exclude": {
    "**/__pycache__": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/.hg": true,
    "**/.mypy_cache": true,
    "**/.sass-cache": true,
    "**/.subl": true,
    "**/.svn": true,
    "**/.vscode": true,
    "**/*.pyc": true,
    "**/bower_components": true,
    "**/node_modules": true,
    "**/tmp": true,
    "**/vendor": true
  },
  "files.insertFinalNewline": true,
  "git.confirmSync": false,
  "gitlens.advanced.messages": {
    "suppressCommitHasNoPreviousCommitWarning": false,
    "suppressCommitNotFoundWarning": false,
    "suppressFileNotUnderSourceControlWarning": false,
    "suppressGitVersionWarning": false,
    "suppressLineUncommittedWarning": false,
    "suppressNoRepositoryWarning": false,
    "suppressResultsExplorerNotice": false,
    "suppressShowKeyBindingsNotice": true,
    "suppressUpdateNotice": false,
    "suppressWelcomeNotice": true
  },
  "gitlens.gitExplorer.files.layout": "auto",
  "gitlens.historyExplorer.enabled": true,
  "gitlens.keymap": "chorded",
  "go.toolsGopath": "~/.go",
  "java.errors.incompleteClasspath.severity": "ignore",
  "ruby.lint": {
    "rubocop": true,
    "ruby": true
  },
  "ruby.interpreter.commandPath": "~/.rvm/rubies/ruby-2.3.1/bin/ruby",
  "ruby.pathToBundler": "~/.rvm/rubies/ruby-2.3.1/bin/bundle",
  "ruby.rctComplete.commandPath": "~/.rvm/rubies/ruby-2.3.1/bin/rct-complete",
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "window.openFilesInNewWindow": "off",
  "window.openFoldersInNewWindow": "on",
  "window.restoreFullscreen": true,
  "window.zoomLevel": -1,
  "workbench.editor.enablePreview": true,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.showTabs": true,
  "workbench.editor.tabCloseButton": "off",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.startupEditor": "none",
  "[ruby]": {
    "editor.formatOnSave": false
  },
  "javascript.updateImportsOnFileMove.enabled": "always",
  "breadcrumbs.enabled": true,
  "editor.renderWhitespace": "all"
}
